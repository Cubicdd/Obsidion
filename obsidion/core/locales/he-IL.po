#: obsidion/core/core_commands.py:32
#, docstring
msgid "Commands related to core functions."
msgstr "פקודות הקשורות לפונקציות ליבה."

#: obsidion/core/core_commands.py:40
#, docstring
msgid "Pong."
msgstr "פונג."

#: obsidion/core/core_commands.py:45
#, docstring
msgid "Shows info about Obsidion."
msgstr "מציג מידע אודות Obsidion."

#: obsidion/core/core_commands.py:65
msgid "This bot is an instance of [Obsidion, an open source Discord bot]({}) created by [Darkflame72]({}) and [improved by many]({}).\n\n"
"Obsidion is backed by a passionate community who contributes and creates content for everyone to enjoy. [Join us today]({}) and help us improve!\n\n"
"(c) Obsidion-dev"
msgstr "רובוט זה הוא מופע של [Obsidion, בוט בקוד פתוח] ({}) שנוצר על ידי [Darkflame72]({}) ו - [המשופר על ידי רבים] ({}).\n\n"
"Obsidion מגובה על ידי קהילה נלהבת שתורמת ויוצרת תוכן עבור כולם ליהנות. [הצטרפו אלינו היום] ({}) ולעזור לנו לשפר!\n\n"
"(ג) Obsidion"

#: obsidion/core/core_commands.py:83
msgid "Bringing joy since 23rd March 2020 (over {} days ago!)"
msgstr "מביא שמחה מאז 23 במרץ 2020 (מעל {} לפני ימים!)"

#: obsidion/core/core_commands.py:91
#, docstring
msgid "Shows Obsidion's uptime."
msgstr "מציג את הזמן של אוסידיון."

#: obsidion/core/core_commands.py:94
msgid "Less than one second"
msgstr "פחות משני"

#: obsidion/core/core_commands.py:96
msgid "Been up for: **{time_quantity}** (since {timestamp} UTC)"
msgstr "היה עד עבור: **{time_quantity}** (מאז {timestamp} UTC)"

#: obsidion/core/core_commands.py:103
#, docstring
msgid "Invite the bot to your server."
msgstr "הזמינו את הרובוט לשרת שלכם."

#: obsidion/core/core_commands.py:105
msgid "You can invite {name} to your Discord server by [clicking here]({invite})."
msgstr "ניתן להזמין {name} לשרת זניחה שלכם על-ידי [לחיצה כאן] ({invite})."

#: obsidion/core/core_commands.py:118
#, docstring
msgid "Leaves the current server."
msgstr "משאיר את השרת הנוכחי."

#: obsidion/core/core_commands.py:119
msgid "Are you sure you want me to leave this server? (y/n)"
msgstr "אתה בטוח שאתה רוצה שאעזוב את השרת? (י/ן)"

#: obsidion/core/core_commands.py:125 obsidion/core/core_commands.py:180
msgid "Response timed out."
msgstr "התגובה חרגה ממגבלת הזמן."

#: obsidion/core/core_commands.py:129
msgid "Alright. Bye :wave:"
msgstr "בסדר. ביי :wave:"

#: obsidion/core/core_commands.py:130
msgid "Leaving guild '{}'"
msgstr "עזיבת הגילדה ' {} '"

#: obsidion/core/core_commands.py:133
msgid "Alright, I'll stay then. :)"
msgstr "בסדר, אני אשאר אז. :)"

#: obsidion/core/core_commands.py:138
#, docstring
msgid "Lists and allows [botname] to leave servers."
msgstr "מפרט ומאפשר [botname] לעזוב שרתים."

#: obsidion/core/core_commands.py:149
msgid "To leave a server, just type its number."
msgstr "כדי להשאיר שרת, הקלידו את המספר שלו."

#: obsidion/core/core_commands.py:164
msgid "I cannot leave a guild I am the owner of."
msgstr "אני לא יכול לעזוב את הגילדה אני הבעלים של."

#: obsidion/core/core_commands.py:168
msgid "Are you sure you want me to leave {}? (yes/no)"
msgstr "את בטוחה שאת רוצה שאעזוב? (כן/לא)"

#: obsidion/core/core_commands.py:176 obsidion/core/core_commands.py:359
#: obsidion/core/core_commands.py:528 obsidion/core/core_commands.py:545
msgid "Done."
msgstr "בוצע."

#: obsidion/core/core_commands.py:178
msgid "Alright then."
msgstr "בסדר גמור אם כך."

#: obsidion/core/core_commands.py:186
#, docstring
msgid "\n"
"        Get info about Obsidion's licenses.\n"
"        "
msgstr "\n"
"        קבל מידע על הרשיונות של Obsidion.\n"
"        "

#: obsidion/core/core_commands.py:191
msgid "This bot is an instance of the Obsidion Discord Bot.\n"
"Obsidion is an open source application made available to the public and licensed under the GNU AGPL v3. The full text of this license is available to you at <https://github.com/Obsidion-dev/Obsidion/blob/master/LICENSE>"
msgstr "רובוט זה הוא מופע של Obsidion Bot.\n"
"Obsidion הוא יישום מקור פתוח אשר זמין לציבור ומורשה תחת ה-GNU AGPL v3. התמליל המלא של רשיון זה זמין לכם בכתובת <https://github.com/Obsidion-dev/Obsidion/blob/master/LICENSE>"

#: obsidion/core/core_commands.py:206
#, docstring
msgid "Displays my full source code or for a specific command.\n\n"
"        To display the source code of a subcommand you can separate it by\n"
"        periods, e.g. account.link for the link subcommand of the account command\n"
"        or by spaces.\n"
"        "
msgstr "הצגת קוד המקור המלא שלי או עבור פקודה ספציפית.\n\n"
"        כדי להציג את קוד המקור של תת-פקודה, ניתן להפריד אותו באמצעות\n"
"        תקופות, לדוגמה account.link עבור תת-הפקודה של הקישור של פקודת החשבון\n"
"        או לפי רווחים.\n"
"        "

#: obsidion/core/core_commands.py:250
#, docstring
msgid "Shuts down the bot."
msgstr "הוא שם למטה את הרובוט."

#: obsidion/core/core_commands.py:261
#, docstring
msgid "Attempts to restart Obsidion.\n\n"
"        Makes [botname] quit with exit code 26.\n"
"        The restart is not guaranteed: it must be dealt\n"
"        with by the process manager in use."
msgstr "ניסיונות להפעיל מחדש את אוסידיון\n\n"
"        עושה [botname] להפסיק עם קוד יציאה 26.\n"
"        הפעלה מחדש אינה מובטחת: יש לטפל ב-\n"
"        עם מנהל התהליך בשימוש."

#: obsidion/core/core_commands.py:274
#, docstring
msgid "Sends to the owner the last command exception that has occurred.\n\n"
"        If public (yes is specified), it will be sent to the chat instead."
msgstr "שולח לבעלים את חריגת הפקודה האחרונה שהתרחשה.\n\n"
"        אם ציבורי (כן מצוין), הוא יישלח לצ ' אט במקום זאת."

#: obsidion/core/core_commands.py:288
msgid "I couldn't send the traceback message to you in DM. Either you blocked me or you disabled DMs in this server."
msgstr "לא יכולתי לשלוח לך את ההודעה. או שאתה חסמת אותי או שאתה מושבת DMs בשרת זה."

#: obsidion/core/core_commands.py:295
msgid "No exception has occurred yet."
msgstr "עדיין לא אירעה חריגה."

#: obsidion/core/core_commands.py:299
#, docstring
msgid "Changes Obsidion's settings."
msgstr "משנה את ההגדרות של אוסידיון."

#: obsidion/core/core_commands.py:304
#, docstring
msgid "\n"
"        Sets a default colour to be used for the bot's embeds.\n\n"
"        Acceptable values for the colour parameter can be found at:\n\n"
"        https://discordpy.readthedocs.io/en/stable/ext/commands/api.html#discord.ext.commands.ColourConverter\n"
"        "
msgstr "\n"
"        הגדרת צבע ברירת מחדל לשימוש עבור המיטות של הרובוט.\n\n"
"        ערכי קבלה עבור פרמטר הצבע ניתן למצוא במיקום:\n\n"
"        https://discordpy.readthedocs.io/en/stable/ext/commands/api.html#discord.ext.commands.Colourממיר\n"
"        "

#: obsidion/core/core_commands.py:314
msgid "The color has been reset."
msgstr "הצבע אופס."

#: obsidion/core/core_commands.py:317
msgid "The color has been set."
msgstr "הצבע הוגדר."

#: obsidion/core/core_commands.py:322
#, docstring
msgid "Sets [botname]'s avatar\n\n"
"        Supports either an attachment or an image URL."
msgstr "הגדרת [botname]של האווטאר\n\n"
"        תומך או קובץ מצורף או URL של תמונה."

#: obsidion/core/core_commands.py:336
msgid "That URL is invalid."
msgstr "ה-URL אינו חוקי."

#: obsidion/core/core_commands.py:339
msgid "Something went wrong while trying to get the image."
msgstr "משהו השתבש בזמן שניסה להשיג את התמונה."

#: obsidion/core/core_commands.py:350
msgid "Failed. Remember that you can edit my avatar up to two times a hour. The URL or attachment must be a valid image in either JPG or PNG format."
msgstr "נכשל. זכור שאתה יכול לערוך את האוואטר שלי עד שתי פעמים בשעה. ה-URL או הצרופה חייבים להיות תמונה חוקית במבנה JPG או PNG."

#: obsidion/core/core_commands.py:357
msgid "JPG / PNG format only."
msgstr "מבנה JPG/PNG בלבד."

#: obsidion/core/core_commands.py:364
#, docstring
msgid "Removes [botname]'s avatar."
msgstr "מסלק את האוואטר של [botname]."

#: obsidion/core/core_commands.py:367
msgid "Avatar removed."
msgstr "אוואטר סולק."

